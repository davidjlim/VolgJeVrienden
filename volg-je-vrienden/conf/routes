# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count


POST    /signin                     controllers.UsersController.signin

POST    /signup                     controllers.UsersController.signup

POST    /updateGPS                  controllers.UsersController.updateGPS

POST    /addImage                   controllers.UsersController.addImage

POST    /getOwnImage                controllers.UsersController.getOwnImage

POST    /getVisibility              controllers.UsersController.getVisibility

POST    /setVisibility              controllers.UsersController.setVisibility


POST    /makeRequest                controllers.RequestsController.makeRequest

POST    /getRequests                controllers.RequestsController.getRequests


POST    /addFriend                  controllers.FriendsController.addFriend

POST    /removeFriend               controllers.FriendsController.removeFriend

POST    /getFriends                 controllers.FriendsController.getFriends
